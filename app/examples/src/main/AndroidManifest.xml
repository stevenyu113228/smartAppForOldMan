<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="ai.olami.android.example">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SpeechInputActivity"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".TextInputWordSegmentationActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".TextInputNLIAnalysisActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".medicineActivity"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".emsActivity"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".foodActivity"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".actActivity"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".easyActivity"
            android:theme="@style/AppTheme1" />
        <!--
        <activity
            android:name="ai.olami.android.sample.MainActivity"
            android:label="@string/title_activity_main"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        -->
        <activity
            android:name="com.journeyapps.barcodescanner.CaptureActivity"
            android:screenOrientation="portrait"
            tools:replace="screenOrientation" />
        <activity
            android:name=".settingActivity"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".loginActivity"
            android:theme="@style/AppTheme1" />

        <activity android:name=".actRegActivity"
            android:theme="@style/AppTheme1"/>


    </application>

</manifest>